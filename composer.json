{
    "name": "garanaw/aquelarre",
    "type": "project",
    "description": "Web version of the Spanish game Aquelarre",
    "keywords": ["aquelarre", "laravel", "rpg", "game", "web", "ddd"],
    "license": "MIT",
    "authors": [
        {
            "name": "Pablo Villalba",
            "email": "garanaw@gmail.com",
            "role": "Developer|Arquitect"
        }
    ],
    "require": {
        "php": "^8.2",
        "doctrine/dbal": "^3.3",
        "guzzlehttp/guzzle": "^7.2",
        "jenssegers/optimus": "^1.1",
        "laravel/fortify": "^1.12",
        "laravel/framework": "^10.0",
        "laravel/horizon": "^5.9",
        "laravel/sanctum": "^3.2",
        "laravel/socialite": "^5.5",
        "laravel/telescope": "^4.14",
        "laravel/tinker": "^2.7",
        "spatie/data-transfer-object": "^3.7",
        "spatie/laravel-fractal": "^6.0",
        "spatie/laravel-permission": "^5.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "fakerphp/faker": "^1.9.1",
        "itsgoingd/clockwork": "^5.1",
        "laravel/pint": "^1.4",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpmd/phpmd": "^2.12",
        "phpunit/phpunit": "^9.5.10",
        "slevomat/coding-standard": "^8.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "files": [
            "app/Core/Framework/Domain/Helpers/helpers.php"
        ],
        "psr-4": {
            "Aquelarre\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
